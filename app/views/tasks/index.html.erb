<h1>Listing tasks</h1>
<%= link_to 'New task', new_task_path %>
<table>
  <tr>
    <th>User</th>
    <th>Email</th>
  </tr>
 
  <% @tasks.each do |task| %>
    <tr>
      <td><%= task.user %></td>
      <td><%= task.email %></td>
      <td><%= link_to 'Show', task_path(task) %></td>
      <td><%= link_to 'Edit', edit_task_path(task) %></td>
      <td><%= link_to 'Destroy', task_path(task),
      		  method: :delete %></td>
    </tr>
  <% end %>
</table>